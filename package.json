{
  "name": "react-msg-box",
  "version": "1.0.6",
  "description": "该react组件包含：alert、confirm、toast三个弹窗组件，并且包含custom自定义组件，支持多层级弹窗形式",
  "repository": {
    "type": "git",
    "url": "http://gitlab.alibaba-inc.com/xunliang.yjm/react-message-box.git"
  },
  "main": "lib/cjs/react-msg-box.js",
  "module": "lib/esm/react-msg-box.js",
  "keywords": [
    "react",
    "message-box",
    "alert",
    "confirm",
    "toast",
    "custom",
    "react-component"
  ],
  "scripts": {
    "dll": "webpack-cli --mode=production --progress --profile --colors --config ./webpack/webpack.dll.conf.js",
    "start": "npm run dev",
    "dev": "webpack-dev-server  --mode=development --progress --profile --colors --config ./webpack/webpack.dev.conf.js",
    "analyze:dev": "webpack-dev-server --env.analyze --mode=development --progress --profile --colors --config ./webpack/webpack.dev.conf.js",
    "build": "webpack-cli --mode=production --progress --profile --colors --config ./webpack/webpack.prod.conf.js",
    "analyze": "webpack-cli --mode=production --env.analyze --progress --profile --colors --config ./webpack/webpack.prod.conf.js",
    "pages": "node ./webpack/pages.js",
    "publish": "node ./rollup/index.js",
    "lint": "prettier --write src/*.jsx",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.jsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-msg-box": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "add-asset-html-webpack-plugin": "^3.0.1",
    "babel-loader": "^8.0.4",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "commander": "^2.19.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "cssnano-preset-advanced": "^4.0.2",
    "file-loader": "^2.0.0",
    "globby": "^6.1.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.4",
    "pascal-case": "^2.0.1",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-px-to-viewport": "^0.0.3",
    "postcss-write-svg": "^3.0.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-hot-loader": "^4.3.11",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "shelljs": "^0.8.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "author": "",
  "license": "ISC"
}
